def Ex1_4(day):
 '''
 (int) -> str

 This function takes the number and outputs the corresponding day number.
 example:
 >>> Ex1_4(5)
 Friday

 '''
 if day==1:
    return "monday"
 if day==2:
    return "tuesday"
 if day==3:
    return "wednesday"
 if day==4:
    return "thursday"
 if day==5:
    return "friday"
 if day==6:
    return "saturday"
 if day==7:
    return "sunday"
 else 
 print("error")
